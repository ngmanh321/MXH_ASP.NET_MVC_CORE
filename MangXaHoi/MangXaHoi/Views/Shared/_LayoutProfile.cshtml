<script>

	function GetFriendGui() {
		var str = '';
		$.ajax({
			type: 'GET',
			url: 'https://localhost:7013/api/api3/0/' + '@tk',
			dataType: 'json',
			success: function (data) {
				$.each(data, function (key, val) {
					trangthai = val.trangThai;
					if(trangthai == 0){
					str += `
									<li>
										<div>
											<figure>
												<img style="width: 45px; height: 45px;" src="../Layout/images/${val.avatarGui}" alt="">
											</figure>
											<div class="mesg-meta">
													<h6><a href="https://localhost:7013/Home/TrangCaNhan?taikhoan=${val.tkgui}" title="">${val.tkgui}</a></h6>
												<span><b>Send</b> friend request to ${val.tknhan}</span>
												<i>Medicine</i>
											</div>
											<div class="add-del-friends">
												<a href="#" title=""><i class="fa fa-heart"></i></a>
												<a href="#" title=""><i class="fa fa-trash"></i></a>
											</div>
										</div>
									</li>
						`
					}
				});
				$('#FriendGui').html(str);

			},

			Error: function (xhr) {
				alert(xhr.resposeText)
			},

		});
	}


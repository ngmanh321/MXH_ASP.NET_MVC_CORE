	function GetMiniChatFriend() {
		var display = '';
		var tknhan = '';
		var str = '';
		$.ajax({
			type: 'GET',
			url: 'https://localhost:7013/api/api3/' + '@tk',
			dataType: 'json',
			success: function (data) {
				$.each(data, function (key, val) {
					tknhan = val.tknhan;
